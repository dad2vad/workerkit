{
  "name": "workerkit",
  "version": "v0.1.0",
  "description": "workerkit",
  "license": "MIT",
  "repository": "",
  "author": {
    "name": "Luca Casonato",
    "email": "hello@lcas.dev",
    "url": "https://lcas.dev"
  },
  "keywords": [
    "cloudflare",
    "cloudflare-worker",
    "typescript",
    "workers-kv"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index",
  "types": "lib/index",
  "scripts": {
    "format": "prettier --write \"{src,__tests__}/**/*.ts\" --single-quote --trailing-comma es5",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "prepublishOnly": "yarn build",
    "prebuild": "yarn format && yarn lint && echo Using TypeScript && tsc --version",
    "build": "tsc --pretty",
    "test": "jest",
    "coverage": "jest --coverage",
    "watch": "yarn build -- --watch",
    "watch:test": "jest --watch",
    "docs": "typedoc --out docs --theme minimal src"
  },
  "dependencies": {
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.3",
    "@types/node": "^10.11.4",
    "coveralls": "^3.0.2",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "ts-jest": "^23.10.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.1.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "jest": {
    "preset": "ts-jest"
  }
}
